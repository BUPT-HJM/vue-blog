@import "reset"
@import "_settings"
@import "_syntax"


[v-cloak]
  display none

html, body
	height 100%
	font-size $body-font-size
	font-family $body-font
	-webkit-font-smoothing antialiased
	-moz-osx-font-smoothing grayscale
	background-color white
input, button
	border-radius $border-radius

